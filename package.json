{
  "name": "chitchat-server",
  "version": "1.0.0",
  "description": "A chatroom web app that allows multiple users to simulateneously communicate via text",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "test": "mocha --reporter list src/**/*.test.js",
    "e2e": "NODE_PATH=src NODE_ENV=test mocha --reporter list e2e/**/*.test.js"
  },
  "keywords": [
    "chat",
    "chatroom",
    "socket",
    "node",
    "express"
  ],
  "author": "Artur J Zapater",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "morgan": "^1.10.0",
    "socket.io": "^2.3.0"
  },
  "devDependencies": {
    "eslint": "^7.10.0",
    "mocha": "^8.1.3",
    "nodemon": "^2.0.4",
    "puppeteer": "^5.3.1",
    "socket.io-client": "^2.3.1",
    "standard": "^14.3.4",
    "supertest": "^5.0.0"
  }
}
